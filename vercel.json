{
  "version": 2,
  "rewrites": [
    {
      "source": "/img/:id*",
      "has": [
        {
          "type": "header",
          "key": "Referer",
          "value": "https://gaehive2.vercel.app/"
        }
      ],
      "destination": "https://uploads.scratch.mit.edu/get_image/project/:id*_300x300.png"
    },
    {
      "source": "/(.*)", 
      "destination": "index.html"
    }
  ]
}